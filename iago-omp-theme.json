{
  "final_space": true,
  "console_title_template": "{{.Folder}}",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "path",
          "style": "diamond",
          "leading_diamond": "\uE0B6",
          "trailing_diamond": "\uE0B0",
          "/powerline_symbol": "\uE0B0",
          "foreground": "#ffffff",
          "background": "#58a6ff",
          "properties": {
            "prefix": " \uE5FF ",
            "style": "folder"
          }
        },
        {
          "type": "git",
          "style": "powerline",
          "powerline_symbol": "\uE0B0",
          "foreground": "#FFFFFF",
          "background": "#6CA35E",
          "properties": {
            "display_status": false,
            "display_stash_count": false,
            "display_upstream_icon": true
          }
        }
      ]
    }
  ]
}
